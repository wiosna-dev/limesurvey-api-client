{
    "name": "wiosna-dev/limesurvey-api-client",
    "description": "Client of LimeSurvey API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Meritoo",
            "email": "github@meritoo.pl"
        }
    ],
    "require": {
        "php": ">=5.6",
        "fguillot/json-rpc": "^1.2",
        "wiosna-dev/common-library": "^0.1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.6",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^4.0",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^5.7",
        "sebastian/phpcpd": "^3.0",
        "squizlabs/php_codesniffer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Meritoo\\LimeSurvey\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meritoo\\LimeSurvey\\Test\\ApiClient\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wiosna-dev/common-library"
        }
    ]
}
